        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "zsalesorder.view",
        "async": true,
        "viewPath": "zsalesorder.view"
      },
      "routes": [
        {
          "name": "RouteSaleOrderList",
          "pattern": ":?query:",
          "target": [
            "TargetSaleOrderList"
          ]
        }
      ],
      "targets": {
        "TargetSaleOrderList": {
          "id": "SaleOrderList",
          "name": "SaleOrderList"
        }
      }
    },
    "rootView": {
      "viewName": "zsalesorder.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
